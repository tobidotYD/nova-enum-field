{
    "name": "datomatic/nova-enum-field",
    "description": "A Laravel Nova PHP 8.1 enum field with filters",
    "keywords": [
        "laravel",
        "nova",
        "enum"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "laravel/nova": "^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Datomatic\\Nova\\Fields\\Enum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
